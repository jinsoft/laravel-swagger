{
    "name": "jinsoft/laravel-swagger",
    "description": "A great way to integrate Swagger into Laravel",
    "keywords": ["laravel", "swagger", "l4", "l5", "documentation", "api"],
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "jinsoft",
            "email": "admin@ainiok.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "zircote/swagger-php": "^2.0"
    },
    "autoload": {
        "psr-0": {
            "Jlapp\\Swaggervel": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jlapp\\Swaggervel\\SwaggervelServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
